{
  "name": "gabrielcuallado",
  "version": "1.0.0",
  "description": "gabrielcuallado's personnal website",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "run-s clean build:parcel build:hugo",
    "start": "run-s clean dev",
    "dev": "run-p dev:**",
    "dev:hugo": "cross-env NODE_ENV=development hugo server -d ../dist -s site -D -F",
    "dev:parcel": "cross-env NODE_ENV=development parcel watch ./site/assets/index.js --no-cache",
    "build:hugo": "cross-env NODE_ENV=production hugo -d ../dist -s site --gc --minify",
    "build:parcel": "cross-env NODE_ENV=development parcel build ./site/assets/index.js --experimental-scope-hoisting  --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexiscolin/GabrielCuallado.git"
  },
  "author": "Alexis Colin (DJDB)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexiscolin/GabrielCuallado/issues"
  },
  "homepage": "https://github.com/alexiscolin/GabrielCuallado#readme",
  "engines": {
    "node": ">= 10.16.0"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0"
  },
  "dependencies": {
    "@barba/core": "^2.9.7",
    "glsl-noise": "0.0.0",
    "gsap": "^3.5.1",
    "imagesloaded": "^4.1.4",
    "minimist": "^1.2.5",
    "modujs": "^1.4.2",
    "node-forge": "^0.10.0",
    "smooth-scrollr": "^0.3.2",
    "three": "^0.118.3",
    "tiny-emitter": "^2.1.0"
  }
}
